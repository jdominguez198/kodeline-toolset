{
  "browser": {
      "proxy": "{{CONFIG_PUBLIC_URL}}",
      "port": "{{CONFIG_BROWSERSYNC_PORT}}"
  },
  "source": {
    "base": "{{CONFIG_THEME_LOCATION}}{{CONFIG_THEME_NAME}}/assets/sass",
    "folders": {
      "core": "core",
      "mixins": "mixins",
      "components": "components"
    },
    "files": {
      "container": "styles.scss"
    }

  },
  "distribution": {
    "base": "{{CONFIG_THEME_LOCATION}}{{CONFIG_THEME_NAME}}/assets/css",
    "file": "{{CONFIG_THEME_NAME}}_styles.min.css"
  }
}
